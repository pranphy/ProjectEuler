#include<bits/stdc++.h>

using namespace std;

bool isPentagonal(long x){
	float tst = 24*x+1;
	cout<<tst<<" is perfect square "<<endl;
	float p = (sqrt(tst)+1)/6.0;
	return int(p) == p;
}

long P(long n){
	return n*(3*n-1)/2.;
}

int main(){
	for( int i = 1; i<119;i++)
		if(isPentagonal(i))
			cout<<i<<" is Pentagonal "<<endl;
}

